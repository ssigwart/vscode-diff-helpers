{
	"publisher": "ssigwart",
	"name": "diff-helpers",
	"displayName": "Diff Helpers",
	"description": "Utilities for working it diffs.",
	"version": "1.0.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ssigwart/vscode-diff-helpers"
	},
	"engines": {
		"vscode": "^1.93.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"git", "diff", "search", "TODOs"
	],
	"activationEvents": [
		"onLanguage:diff"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "diff-helpers.searchDiff",
				"title": "Search Diff",
				"enablement": "editorLangId == 'diff'"
			},
			{
				"command": "diff-helpers.searchGitDiff",
				"title": "Git: Search Diff"
			},
			{
				"command": "diff-helpers.searchGitCachedDiff",
				"title": "Git: Search Cached Diff"
			},
			{
				"command": "diff-helpers.searchBranchDiff",
				"title": "Git: Search Branch Diff"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/vscode": "^1.93.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"@typescript-eslint/eslint-plugin": "^6.19.1",
		"@typescript-eslint/parser": "^6.19.1",
		"eslint": "^8.56.0",
		"typescript": "^5.3.3",
		"ts-loader": "^9.5.1",
		"webpack": "^5.90.0",
		"webpack-cli": "^5.1.4"
	}
}
